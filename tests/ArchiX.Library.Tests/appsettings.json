{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Observability": {
    "Enabled": true,
    "Tracing": {
      "Enabled": true,
      "Exporter": "none"
    },
    "Metrics": {
      "Enabled": true,
      "Exporter": "prometheus",
      "ScrapeEndpoint": "/metrics"
    },
    "Logs": {
      "Enabled": true,
      "Exporter": "none"
    }
  },
  "ArchiX": {
    "AllowDbOps": false,
    "AllowDbOps_Description": "CK Not: true ise Database CRUD işlemleri kodları çalışır. örneğin library veya test ortamında zaten migration ile oluştuğu için false olur. Ama Prod ortamında ise DB kodları çalışması gerekir."
  },
  "ExternalServices": {
    "Ping": {
      "BaseAddress": "http://localhost:5000",
      "TimeoutSeconds": 30
    }
  }
}
